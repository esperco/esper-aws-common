OCAML_LIBS = $(OCAML_LIBS_aws-common)

FILES[] =
  aws_common_t
  aws_common_j
  aws_common_v
  aws_common

Atdgen(aws_common, -j-std)
OCamlLibrary(aws-common, $(FILES))
Meta(aws-common)

.DEFAULT: META aws-common.cma aws-common.cmxa

.PHONY: clean
clean:
  rm -f *.o *.a *.cm* *~ *.annot *.run *.opt META $(ATDGEN_OUTFILES)
